{
    "name": "armincms/sofre-api",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/sofre": "~0.2",
        "armincms/snail": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\SofreApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\SofreApi\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
